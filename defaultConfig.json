{
  "Basic": {
    "LEVEL": "INFO",
    "FORMAT": "[%(levelname)s] %(asctime)s <%(name)s> %(pathname)s line:%(lineno)d :%(message)s"
  },
  "Console": {
    "LEVEL": "DEBUG",
    "FORMAT": "[%(levelname)s] %(asctime)s <%(name)s> %(pathname)s line:%(lineno)d :%(message)s"
  },
  "File": {
    // 当这里没有 "LEVEL" 或 "FORMAT" 时将使用 Basic中的配置
    // "TYPE":"Ordinary" or "Rotating"
    "TYPE": "Rotating",
    "FILE_PATH": "./logs/log",
    //当"TYPE":"Ordinary" 时，以下两个配置不需要写
    "MAX_BYTES": 5,
    "BACKUP_COUNT": 3
  },
  "mongodb": {
  }
}